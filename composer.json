{
    "name": "webreports/shopify",
    "description": "The WebReports Shopify Framework",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=7.0",
        "adbario/php-dot-notation": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "6"
    },
    "autoload": {
        "psr-4": {
            "WebReports\\Shopify\\": "src/Shopify/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
